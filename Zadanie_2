weather = ['в', '5', 'часов', '17', 'минут', 'температура', 'воздуха', 'была', '+5', 'градусов']
weather.insert(1,'"')
weather.insert(3,'"')
weather.insert(5,'"')
weather.insert(7,'"')
weather.insert(12,'"')
weather.insert(14,'"')
weather.remove('5')
weather.insert(2,'05')
weather.remove('+5')
weather.insert(13,'+05')
weather_1 = " ".join(weather)
print(weather_1)
